module.exports = {
    read: function(cardType, successCallback, failureCallback) {
        cordova.exec(successCallback,
            failureCallback,
            "Main",
            "read",
            [cardType]);
    },
    sleep: function(successCallback, failureCallback) {
        cordova.exec(successCallback,
            failureCallback,
            "Main",
            "sleep",
            []);
    }
};
